        <externalId>false</externalId>
        <formula>TEXT( YEAR(Excpected_Billing_Date__c) ) &amp; &apos; - &apos; &amp; CASE(MONTH(  Excpected_Billing_Date__c  ) ,
	1, &apos;01&apos;,
	2, &apos;02&apos;,
	3, &apos;03&apos;,
	4, &apos;04&apos;,
	5, &apos;05&apos;,
	6, &apos;06&apos;,
	7, &apos;07&apos;,
	8, &apos;08&apos;,
	9, &apos;09&apos;,
	10, &apos;10&apos;,
	11, &apos;11&apos;,
	12, &apos;12&apos;,
        &apos;&apos;)</formula>
