    <validationRules>
        <fullName>Bill_Percentage_check</fullName>
        <active>true</active>
        <errorConditionFormula>Bill__c+Billed__c&gt;1</errorConditionFormula>
        <errorDisplayField>Bill__c</errorDisplayField>
        <errorMessage>Invalid Bill (%). Please provide Bill (%) according to Billed amount.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Time_Frame_Numeric_Check</fullName>
        <active>true</active>
        <description>Time frame must only accept numeric values except for Invoice record having Retainer stage.</description>
        <errorConditionFormula>Name  != &apos;Retainer&apos; &amp;&amp;  NOT(ISNUMBER(Time_Frame__c)) &amp;&amp; Time_Frame__c &lt;&gt; null</errorConditionFormula>
        <errorDisplayField>Time_Frame__c</errorDisplayField>
        <errorMessage>Time frame should be a number.</errorMessage>
    </validationRules>
