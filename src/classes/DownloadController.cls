public class DownloadController {
    public List<SelectOption> lst_NamesToExport {get; set;}
    Id idValue ;
    public SObject sObjects {get;private set;}
    public List <String> fields {get; private set;}
    public List <String> LstObject {get; private set;}
    public String fieldValues {get; private set;}
    public String fieldNames {get; private set;}
    public String selectedObject {get; set;}
    
    public DownloadController(){
        
    } 
    public void initilizeDownloadController(){
         
        fieldNames='';
        fieldValues='';
        fields = new List <String>();
        LstObject = new List <String>();
        idValue =ApexPages.currentPage().getParameters().get('id');

        String objectAPIName = idValue.getSObjectType().getDescribe().getName();
        SObjectType accountType = Schema.getGlobalDescribe().get(objectAPIName);
        Map<String,Schema.SObjectField> mfields = accountType.getDescribe().fields.getMap();
        String query = 'Select ';
        Integer anyField = 0;

        for(String key :mfields.keyset()){
            SObjectField field = mfields.get(key);
            if(field.getDescribe().isAccessible()){
                query= query +mfields.get(key)+ ',';   
                anyField=1;
                fields.add(String.valueOf(mfields.get(key)));
                String fName = field.getDescribe().getLabel();
                fieldNames =  fieldNames+'"'+fName+'"' + ' ,'; 
              
            } 
        }            

        //Execute only if any field is accesible
        if(anyField == 1){
            query = query.removeEnd(',');  
            fieldNames = fieldNames.removeEnd(','); 
            query = query+ ' from '+objectAPIName + ' where id = \''+idValue+'\'';
            sObjects = Database.query(query); 
            String EmailFinal = '';
            for(String key:fields){
                if(String.valueOf(sObjects.get(key)) != null){
                   
                    LstObject.add(String.valueOf(sObjects.get(key)));
                    String tmpValue = String.valueOf(sObjects.get(key));
                    String finalValue = tmpValue.replaceAll('"','\'');
                    fieldValues  = fieldValues+'"'+finalValue+'"' +','; 
                     EmailFinal= EmailFinal + key +' ( '+finalValue+' )'+' ,';
                }
                else{
                    LstObject.add('\' \'');
                    fieldValues  = fieldValues+'" "'+',';    
                      EmailFinal= EmailFinal + key +' ( "" )'+' ,';
                }
            }
          
            fieldValues = fieldValues.removeEnd(','); 
        }
    }
    
