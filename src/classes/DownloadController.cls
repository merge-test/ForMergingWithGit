    }
    public PageReference resetOpp(){
        
        return redirectToRecord();
    }

    public PageReference redirectToRecord(){
        System.debug('Value of Id '+idValue);
        PageReference reference = new PageReference('/'+idValue);
        reference.setRedirect(true);
        return reference;
    } 

    
